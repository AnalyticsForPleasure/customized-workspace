<snippet>
	<content><![CDATA[
function max()
{
	if [ $1 -gt $2 ]
	then
		echo $1
	else
		echo $2
}

my_number1=56
my_number2=89

larger=$(max $my_number1 $my_number2) #Invoking the function

echo "$larger is a larger number"
]]></content>
	<tabTrigger>function</tabTrigger>
	<scope>source.shell</scope>
</snippet>
